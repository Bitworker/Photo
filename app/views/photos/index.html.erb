<ul><%= render :partial => 'photos/thumb',:collection => @photos, :as => :photo %></ul>

<%= content_for :action_links do %>
<%= link_to "Zeige Alben die Fotos mit folgenen Tags besitzen: #{params[:q]}", albums_path(:q => params[:q]) if params[:q] %>
<%= link_to "Zeige alle Fotos die folgende Tags besitzen: #{params[:tag_id]}", tag_photos_path(params[:tag_id]) if params[:tag_id] && params[:album_id] %>
<% end %>
